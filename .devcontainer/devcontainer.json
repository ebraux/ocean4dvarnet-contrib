{
    "name": "My DevContainer",
    "image": "mambaorg/micromamba:latest",
    "build": {
      "context": ".",
      "dockerfile": "Dockerfile"
    },
    "postCreateCommand": "mamba install -y -c conda-forge pytorch",
    "settings": {
      "python.pythonPath": "/usr/local/bin/python"
    },
    "extensions": [
      "ms-python.python",
      "ms-vscode.cpptools"
    ],
    "remoteUser": "vscode"
  }
  